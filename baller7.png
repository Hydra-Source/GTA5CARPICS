	function hwid_tab()
	{
		window.location.href = "https://cherax.vip/store/cherax.1/#hwid";
		document.getElementsByClassName("tabs-tab")[0].style.borderBottomColor = "transparent";
		document.getElementsByClassName("tabs-tab")[1].style.borderBottomColor = "transparent";
