		hwid_url_request(); 
	}
	
	function hwid_timer()
	{
		setTimeout(function()
				{
					var displayTimeIn = document.getElementById("hwid_timer");
					var timeDetect = displayTimeIn.innerHTML;
					var time = timeDetect.split(":");
					var hours = parseInt(time[0]);
					var min = parseInt(time[1]);
					var sec = parseInt(time[2]);
					var totalSeconds = sec + min * 60 + hours * 60 * 60;
					setInterval(function(e)
					{
						totalSeconds -= 1;
						if (totalSeconds < 0)
							totalSeconds = 0;
						hours = 0;
						min = 0;
						sec = 0;
						
						var tmpTotal = totalSeconds;
						
						while (tmpTotal >= 3600)
						{
							 tmpTotal -= 3600;
							 hours++;
						}

						if(hours < 10)
							hours = String(hours).padStart(2, '0');
						
						while (tmpTotal >= 60)
						{
							tmpTotal -= 60;
							min++;
						}

						if(min < 10)
							min = String(min).padStart(2, '0');

						sec = tmpTotal;

						if(sec < 10)
							sec = String(sec).padStart(2, '0');

						displayTimeIn.innerHTML = hours + ":" + min + ":" + sec;

					}, 1000);


				}, 0);	
	}
		
	function hwid_url_request()
	{
		$.get("https://cherax.vip/reset_hwid.php", function( data ) 
		{	
			document.getElementsByClassName("block-body")[1].innerHTML = data;
			hwid_timer();
		});	
	}
		
	function call_reset_script()
	{
		$.get("https://cherax.vip/loader_reset_hwid.php", function( data ) 
		{
			alert(data);
			window.location.reload(false); 
		});
	}		
	
</script>
